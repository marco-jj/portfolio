{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@data/*": ["src/data/*"],
			"@assets/*": ["src/assets/*"],
			"@utils/*": ["src/utils/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"]
}
